{
  "name": "head-starter",
  "version": "1.1.6",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lintfix": "eslint \"src/**/*.ts\" --fix",
    "test": "ava",
    "posttest": "npm run view-coverage && npm run report && npm run lint",
    "prepack": "rm -rf lib && tsc -b",
    "commit": "git-cz",
    "report": "nyc report --reporter=json",
    "semantic-release": "semantic-release",
    "view-coverage": "nyc --extension .ts ava --forbid-only"
  },
  "keywords": [
    "ts-packrat",
    "ts-packrat",
    "geenee"
  ],
  "author": "YizYah",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.14.39",
    "bash-fool": "^1.0.0",
    "chalk": "^4.1.0",
    "del": "^6.0.0",
    "dynamapping": "^1.6.0",
    "execa": "^4.1.0",
    "fs-extra": "^9.1.0",
    "installist": "^1.1.0",
    "interactic": "^1.0.0",
    "js-yaml": "^4.1.0",
    "listr": "^0.14.3",
    "magicalstrings": "^0.0.14",
    "path": "^0.12.7",
    "proxyquire": "^2.1.3",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "ava": "^3.15.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.24.0",
    "ghooks": "^2.0.4",
    "inquirer": "^6.5.2",
    "mock-fs": "^4.13.0",
    "mock-require": "^3.0.3",
    "mock-stdin": "^1.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2",
    "sinon": "^9.2.3",
    "stdout-stderr": "^0.1.13",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "bugs": "https://github.com/YizYah/head-start/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/YizYah/head-start",
  "types": "lib/index.d.ts",
  "repository": "YizYah/head-start",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*.test.ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
